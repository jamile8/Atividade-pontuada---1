programa {
  funcao inicio() {
    
   cadeia cor
    real preco

    escreva("Digite a cor que você deseja achar: ")
    leia(cor)

    se (cor == "verde")
    preco = 10.00

    se (cor == "azul")
    preco = 20.00

    se (cor == "amarelo")
    preco = 30.00

    se (cor == "vermelho")
    preco = 40.00

    escreva("\nPreço: " + preco)



  }

}
